    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://stream-ontology.com/nomadmetainfo#hasNumberOfDimensions"/>
      <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:hasValue>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://stream-ontology.com/nomadmetainfo#hasVarDim1"/>
      <owl:someValuesFrom rdf:resource="http://stream-ontology.com/nomadmetainfo#number_of_species_projected_dos_values"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Contains the set of discrete energy values with respect to the top of the valence band for the species-projected density of states (DOS). It is derived from the species_projected_dos_energies species field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://stream-ontology.com/nomadmetainfo#species_projected_dos_energies">
  <rdfs:subClassOf rdf:resource="http://stream-ontology.com/nomadmetainfo#NMI_concrete"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://stream-ontology.com/nomadmetainfo#hasDatatype"/>
