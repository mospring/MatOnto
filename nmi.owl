  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://stream-ontology.com/nomadmetainfo#hasReference"/>
      <owl:someValuesFrom rdf:resource="http://stream-ontology.com/nomadmetainfo#section_system"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference to the system (atomic configuration, cell, ...) that is calculated in section_single_configuration_calculation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://stream-ontology.com/nomadmetainfo#single_configuration_to_calculation_method_ref">
