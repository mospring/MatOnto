      <owl:allValuesFrom rdf:resource="http://stream-ontology.com/nomadmetainfo#integer"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the International Union of Crystallography (IUC) number of the 3D space group of this system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://stream-ontology.com/nomadmetainfo#species_projected_dos_energies_normalized">
