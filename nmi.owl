<owl:Class rdf:about="http://stream-ontology.com/nomadmetainfo#atomic_isotropic_polarizability">
  <rdfs:subClassOf rdf:resource="http://stream-ontology.com/nomadmetainfo#NMI_concrete"/>
  <rdfs:subClassOf>
    <owl:Restriction>
