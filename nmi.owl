    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://stream-ontology.com/nomadmetainfo#hasUnit"/>
      <owl:allValuesFrom rdf:resource="http://stream-ontology.com/nomadmetainfo#Meter"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://stream-ontology.com/nomadmetainfo#hasAbstractType"/>
